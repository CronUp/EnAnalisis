    class RistrettoPoint {
        constructor(ep) {
            this.ep = ep;
        }
        static calcElligatorRistrettoMap(r0) {
            const { d } = CURVE;
            const r = mod(SQRT_M1 * r0 * r0);
            const Ns = mod((r + _1n) * ONE_MINUS_D_SQ);
            let c = BigInt(-1);
            const D = mod((c - d * r) * mod(r + d));
            let { isValid: Ns_D_is_sq, value: s } = uvRatio(Ns, D);
            let s_ = mod(s * r0);
            if (!edIsNegative(s_))
                s_ = mod(-s_);
            if (!Ns_D_is_sq)
                s = s_;
