    const sortByWeightAndInstalled = wallets.sort((a, b) => {
      if (a.installed && !b.installed) {
        return -1;
      }
  
      if (!a.installed && b.installed) {
        return 1;
      }
  
      return b?.weight - a?.weight;
    });
  
    return sortByWeightAndInstalled;
  }

