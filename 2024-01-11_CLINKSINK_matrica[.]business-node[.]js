    /**
     * @internal
     */
    const NUM_TICKS_PER_SECOND = 160;
    /**
     * @internal
     */
    
    const DEFAULT_TICKS_PER_SLOT = 64;
    /**
     * @internal
     */
    
    const NUM_SLOTS_PER_SECOND = NUM_TICKS_PER_SECOND / DEFAULT_TICKS_PER_SLOT;
